{
    "coc.preferences.formatOnSaveFiletypes": ["dart"],
    "languageserver": {
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod"],
            "filetypes": ["go"],
            "trace.server": "verbose",
            "initializationOptions": {
                "usePlaceholders": true
            }
        },
        "terraform": {
            "command": "terraform-lsp",
            "filetypes": [
                "terraform",
                "tf"
            ],
            "initializationOptions": {},
            "settings": {}
        }
    }
}
